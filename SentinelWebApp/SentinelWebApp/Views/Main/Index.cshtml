                }
            });

            return exists;
        }

        function CompareSerials(s) {
            var arr = [];

            $.each($('#serial-exists-list li'), function (i, v) {
                var temp = $(v).text().trim();
                arr.push(temp);

                if (s == $(v).text().trim()) {

                    $(v).addClass("greenBg");

                    $('#serial-exists-list').find('#errorItem').remove();
                }
            });

            if (jQuery.inArray(s, arr) == -1) {

                var num = parseInt($('#totalItem').text());

                if (!isNaN(num)) {

                    if ($('#serial-exists-list').children().length > num) {
                        $('#errorItem').replaceWith(`<li id="errorItem" class="list-group-item redBg">
