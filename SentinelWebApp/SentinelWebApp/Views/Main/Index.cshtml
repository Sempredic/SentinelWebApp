        function RemoveSerial(s) {

            var activeTab = $('ul#vcTabs').find('li.active').data('shit');
            var arr = [];

            if (activeTab == "verifyTab") {

                $.each($('#serial-exists-list li'), function (i, v) {
                    var temp = $(v).text().trim();
                    arr.push(temp);

                    if ($(s).text().trim() == $(v).text().trim()) {

                        $(v).removeClass("greenBg");

                    }
                });

                $('#serial-exists-list').find('#errorItem').remove();

                $(s).remove();

                $('#addedItem').text($('#serial-added-list').children().length);
            } else {

                var result = $.ajax({
                    type: "POST",
                    url: "/Main/RemoveSerial",
                    data: {
                        serialID: $(s).text().trim()
                    },
                    async: false,
                    dataType: 'json',
                    success: function (data) {

                        alert("Success");

                    },
                    error: function (response) {
                        alert('eror');
                    }
                }).responseJSON;

                result = JSON.stringify(result);

                alert(result);

                if (result == "true") {
                    $(s).remove();

                    PopulateSerialLists($('#caseIDInput').val());


                }

            }


        }

        function CompareAddSerial(s) {

            var exists = false;

            $.each($('#serial-added-list li'), function (i, v) {
                var temp = $(v).text().trim();

                if (s == $(v).text().trim()) {

                    exists = true;
                }
            });

            return exists;
        }

        function CompareExistsSerial(s) {
            var exists = false;

            $.each($('#serial-exists-list li'), function (i, v) {
                var temp = $(v).text().trim();

                if (s == $(v).text().trim()) {

